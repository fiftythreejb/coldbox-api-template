{
    "name":"api-cb-template",
    "web":{
        "host":"127.0.0.1",
        "http":{
            "port":"8080"
        }
    },
	"cacheDefaultObject":"coldbox",
    "debuggingEnabled":true,
    "debuggingReportExecutionTimes":false,
    "disableInternalCFJavaComponents":false,
	"requestTimeoutEnabled":true,
	"requestTimeout":"0,0,5,0",
	"robustExceptionEnabled":true,
    "caches":{
		"coldbox":{
			"storage":"true",
            "type":"RAM",
            "custom":{
				"timeToIdleSeconds":"1800",
                "timeToLiveSeconds":"3600"
            },
            "class":"lucee.runtime.cache.ram.RamCache",
            "readOnly":"false"
        }
    },
	"whitespaceManagement":"white-space-pref",
	"datasources":{
		"Documents":{
            "blob":"false",
            "class":"oracle.jdbc.OracleDriver",
			"bundleName": "org.lucee.oracle", 
			"bundleVersion": "21.8.0.0-ojdbc8",
            "dbdriver":"Oracle",
            "clob":"false",
            "connectionLimit":"-1",
            "connectionTimeout":"20",
            "host":"${DOCUMENTS_HOST}",
            "port":"${DOCUMENTS_PORT}",
            "database":"${DOCUMENTS_DATABASE}",
            "connectionString":"jdbc:oracle:thin:@{host}:{port}:{database}",
            "username":"${DOCUMENTS_USERNAME}",
            "password":"${DOCUMENTS_PASSWORD}",
            "validate":"true"
        }
	}
}
